@model Art_Gallery.Models.Tbl_Art

@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>DeleteArt</title>
</head>
<body>
    <h3>Are you sure you want to delete this?</h3>
    <div>
       
        <hr />
        <dl class="dl-horizontal" style="padding-left:20px">
            <dt>
                @Html.DisplayName("Art Name")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.Art_Name)
            </dd>
    
            <dt>
                @Html.DisplayName("Art Description")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.Art_Desc)
            </dd>
    
            <dt>
                @Html.DisplayName("Art Price")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.Art_Price)
            </dd>
    
            <dt>
                @Html.DisplayName("Art Image")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.Art_Image)
            </dd>
    
            <dt>
                @Html.DisplayName("Active")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.IsActive)
            </dd>
    
            
    
            <dt>
                @Html.DisplayName("Created Date")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.CreatedDate)
            </dd>
    
            <dt>
                @Html.DisplayName("Modified Date")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.ModifiedDate)
            </dd>
    
            <dt>
                @Html.DisplayName("Category")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.Tbl_Category.Category_Name)
            </dd>
    
            <dt>
                @Html.DisplayName("Artist Name")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.Artist_Name)
            </dd>
    
        </dl>
    
        @using (Html.BeginForm()) {
            @Html.AntiForgeryToken()
    
            <div class="form-actions no-color">
                <input type="submit" value="Delete" class="btn btn-default" /> |
                @Html.ActionLink("Back to List", "Art")
            </div>
        }
    </div>
</body>
</html>
