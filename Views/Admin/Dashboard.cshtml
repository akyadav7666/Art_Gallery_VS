        <div class="row" style="margin:20px">
            <div class="col-md-4">
                <div class="card card-chart">
                    <div class="card-header card-header-success">
                        <div class="ct-chart" id="dailySalesChart"></div>
                    </div>
                    <div class="card-body">
                        <h4 class="card-title">Custom Art Request</h4>
                        <h5 class="card-category">
                            @car
                        </h5>
                    </div>
                    <div class="card-footer">
                        <div class="stats">
                            <i class="material-icons">Users Art Requests !!!</i>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="card card-chart">
                    <div class="card-header card-header-warning">
                        <div class="ct-chart" id="websiteViewsChart"></div>
                    </div>
                    <div class="card-body">
                        <h4 class="card-title">Report</h4>
                        <h5 class="card-category">@report</h5>
                    </div>
                    <div class="card-footer">
                        <div class="stats">
                            <i class="material-icons">Users Report !!!</i>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="card card-chart">
                    <div class="card-header card-header-danger">
                        <div class="ct-chart" id="completedTasksChart"></div>
                    </div>
                    <div class="card-body">
                        <h4 class="card-title">Feedback</h4>
                        <h5 class="w3-text-blue">@feedback</h5>
                    </div>
                    <div class="card-footer">
                        <div class="stats">
                            <i class="material-icons">Users Feedback !!!</i>
                        </div>
                    </div>
                </div>
            </div>
        </div>


        <div class="row" style="">

            <div class=" col-md-6 ">
                <div class="card">
                    <div class="card-header card-header-warning">
                        <h4 class="card-title">Art Stats</h4>
                        <p class="card-category">New Art on @DateTime.Now</p>
                    </div>

                    <div class="card-body table-responsive">

                        <table class="table table-hover">
                            <thead class="text-warning">
                            <th>ID</th>
                            <th>Name</th>
                            <th>Price</th>
                            <th>Artist</th>
                            </thead>
                            <tbody>
                                @foreach (var item in Model.Tbl_Art)
                                {
                                    <tr>
                                        <td>@item.Art_Id</td>
                                        <td>@item.Art_Name</td>
                                        <td>$@item.Art_Price</td>
                                        <td>@item.Artist_Name</td>
                                    </tr>

                                }

                            </tbody>
                        </table>
                    </div>
                </div>

            </div>



            <div class=" col-md-6">
                <div class="card">
                    <div class="card-header card-header-warning">
                        <h4 class="card-title">Category Stats</h4>
                        <p class="card-category">New Category on @DateTime.Now</p>
                    </div>

                    <div class="card-body table-responsive">

                        <table class="table table-hover">
                            <thead class="text-warning">
                            <th>ID</th>
                            <th>Name</th>
                            <th>Status</th>
                            <th>Image</th>
                            </thead>
                            <tbody>
                                @foreach (var item in Model.Tbl_Category)
                                {
                                    <tr>
                                        <td>@item.Category_Id</td>
                                        <td>@item.Category_Name</td>
                                        <td>@item.Category_Status</td>
                                        <td><img src="@Url.Content(item.Category_Image)" style="height:30px;width:30px;" /></td>
                                    </tr>

                                }

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>





